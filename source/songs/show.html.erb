<% song = data.songs[letter] %>

<div class="banner padded" style="background-image:linear-gradient(rgba(250,250,250,0.2), rgba(250,250,250,0.2)), url(<%=image_path song.image %>); background-size: cover; height: 450px; background-position: center;">
 </div>

<div class="songy">
<iframe src= <%= song.url %>
        width="250" height="80" frameborder="0" allowtransparency="true"></iframe>
</div>

<div class="jumbotron" id="jumbo1">
  <div class="wrapper">
    <div class="row">
      <div class="col-md-6">
      <hr>
      <div class="row">
        <div class="col-md-6">
          <h4 id="noshadow">Artist:</h4>
        </div>
        <div class="col-md-6">
          <p class="pull-right" id="noshadow"><%= song.artist %></p>
        </div>
      </div>
      <hr>
      <div class="row">
        <div class="col-md-6">
          <h4 id="noshadow">Genre:</h4>
        </div>
        <div class="col-md-6">
          <p class="pull-right" id="noshadow"><%= song.genre %></p>
        </div>
      </div>
      <hr>
      <br>
      <div class="row">
      <div class="col-md-10">

      </div>
        <div class="col-md-2">
          <div class="fb-share-button" data-href= <%= song.data %> data-layout="button" data-size="large" data-mobile-iframe="true"><a class="fb-xfbml-parse-ignore" target="_blank" href= <%= song.share %> >Share</a></div>
      </div>
      </div>
    </div>
  </div>
</div>
</div>
